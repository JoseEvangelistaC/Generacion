<div id="observacionesConsola" style="display: flex; padding: 10px 0 0 0;">
    <span style="padding: 0 10px 0 0;">Observaciones</span>
    <textarea style="height: 113px; width: 90%;">@*@datoFormato.Observaciones*@</textarea>
</div>
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script>

    $(document).ready(function () {

        let estadoActual = 1;

        $("#EG1").click(function () {
            if (estadoActual === 1) {
                $("#datos-EG1").hide();
                $("#titulo-EG1").hide();
                $("#datos-EG2").show();
                $("#titulo-EG2").show();
                $("#EG1").text("EG02");
                estadoActual = 2;
            } else if (estadoActual === 2) {
                $("#datos-EG2").hide();
                $("#titulo-EG2").hide();
                $("#datos-EG1").show();
                $("#titulo-EG1").show();
                $("#EG1").text("EG01");
                estadoActual = 1;
            }
        });

    });


    function ObtenerDatosFormato() {
        var idReporte = datosOperario.IdSitio + "-ELD.CTL-OM-001-" + obtenerFecha("fecha");
        var filasIds = @Html.Raw(JsonConvert.SerializeObject(idReferencias));

        const datos = [];

        for (let j = 1; j <= 2; j++) {
            filasIds.forEach(filaId => {
                const filaPrincipal = document.querySelector(`[data-id^="${filaId}-G${j}"]`);

                const cantidadElementos = (filaPrincipal.children.length - 3) * 2;
                console.log("cantidadElementos : ", cantidadElementos);

                for (let i = 2; i <= cantidadElementos; i = i + 2) {
                    var idDetalle = datosOperario.IdSitio + "-Det-" + filaId + "-" + j.toString() + "-" + i.toString() + "-" + obtenerFecha("fecha");

                    const celda = filaPrincipal.querySelector(`td[id="${filaId}_${i}"]`);
                    console.log("celda : ", celda);

                    const dato = {
                        idDetalleCampo: idDetalle,
                        detalle: parseFloat(celda.querySelector('input').value || 0),
                        fila: i,
                        numeroGenerador: j,
                        hora: i + ':00',
                        sitio: datosOperario.IdSitio,
                        idSubtituloCampo: filaId,
                        fecha: obtenerFecha('fechaVista'),
                        idReporteCampo: idReporte
                    };

                    datos.push(Object.assign({}, dato));
                }
            });
        }


        loader.style.display = 'block';

        return fetch('@Url.Action("GuardarDatosConsola", "LecturasCampo")' , {
            method: "POST",
            headers: {
                "Content-Type": "application/json"
            },
            body: JSON.stringify(datos)
        })
            .then(function (response) {
                console.log('ejecutado correctamente: ', response);
                return response.json();
            })
            .then(function (data) {
                loader.style.display = 'none';

                if (data.respuesta.idRespuesta === 0 || data.respuesta.idRespuesta === 1) {
                    Swal.fire({
                        title: "Success!!",
                        text: "Se guardaron los datos.",
                        icon: "success",
                        button: "Aww yiss!",
                        willClose: () => {
                            location.reload();
                        }
                    });

                } else {
                    Swal.fire({
                        title: "Error",
                        text: data.respuesta.mensaje,
                        icon: "error",
                        button: "OK",
                    });
                }
            })
            .catch(function (error) {
                loader.style.display = 'none';
                console.error("Error:", error);
            });
    }


    $(document).ready(function () {
        $("#GuardarDatos").click(function () {
            ObtenerDatosFormato()
        });
    });


    for (let i = 1; i <= 10; i++) {
        const principalElement = document.querySelector(`.principal${i}`);
        const tablaEG = document.querySelector(`.boton${i}`);

        principalElement.addEventListener("click", () => {
            if (tablaEG.style.display === "none" || tablaEG.style.display === "") {
                tablaEG.style.display = "table";
            } else {
                tablaEG.style.display = "none";
            }
        });
    }

    $(document).ready(function () {
        $("#accordionSidebar").addClass("toggled");
    });





    const thElements = document.querySelectorAll('th');

    thElements.forEach(th => {
        th.addEventListener('mouseover', function () {
            const info = this.getAttribute('data-info');
            if (info != "" && info != null) {
                const tooltip = document.createElement('div');
                tooltip.textContent = info;

                tooltip.style.position = 'absolute';
                tooltip.style.backgroundColor = 'rgba(0, 0, 0, 0.7)';
                tooltip.style.color = 'white';
                tooltip.style.padding = '5px';
                tooltip.style.borderRadius = '5px';

                tooltip.style.left = (this.getBoundingClientRect().left + window.scrollX) + 'px';
                tooltip.style.top = (this.getBoundingClientRect().top + window.scrollY - 30) + 'px';

                document.body.appendChild(tooltip);

                this.addEventListener('mouseout', function () {
                    tooltip.remove();
                });
            }
        });
    });
</script>